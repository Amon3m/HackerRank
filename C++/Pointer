#include <stdio.h>
#include <iostream>

using namespace std;
void update(int *a,int *b) {
 int sum = *a+*b;
  int abs;
  if (*a>*b){
 abs= *a-*b;


  }
 else if (*b>*a)
{


          abs= *b-*a;

}
 cout<<sum<<endl;

 cout <<abs<<endl;


}

int main() {

    int a, b;
    int *pa = &a, *pb = &b;
    scanf("%d %d", &a, &b);
    update(pa, pb);
   // printf("%d\n%d", a, b);

    return 0;
}
